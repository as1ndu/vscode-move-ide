{
  "name": "vscode-move-ide",
  "version": "0.0.1",
  "description": "VSCode extension for Move language",
  "main": "./extension.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "vscode": "^1.43.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "move",
        "extensions": [
          ".mvir",
          ".move"
        ]
      }
    ],
    "grammars": [
      {
        "language": "move",
        "scopeName": "source.move",
        "path": "./syntaxes/move.tmLanguage.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/damirka/vscode-move-ide.git"
  },
  "keywords": [
    "libra",
    "move",
    "dfinance",
    "ide",
    "move-ide",
    "libra-ide",
    "dfinance-ide"
  ],
  "author": "Damir Shamanaev <damirka.ru@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/damirka/vscode-move-ide/issues"
  },
  "homepage": "https://github.com/damirka/vscode-move-ide#readme",
  "devDependencies": {
    "@types/vscode": "^1.43.0"
  }
}
